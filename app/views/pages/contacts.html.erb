<div class="row">
  <div class="heading-wrapper">
    <h1>Наши контакты</h1>
  </div>
</div>

<div class="row">
  <ul class="contacts-list d-flex flex-column flex-md-row justify-content-between">
  
    <% @contacts.each do |cont| %>
      <li>
      <%= image_tag( cont[:avatar] , { width: '100px' }) %><br>
      <%= cont[:name_label] %><%= cont[:name] %><br> 
      <%= cont[:email_label] %><%= mail_to cont[:email] %><br> 
      <%= cont[:tel_label] %><%=  link_to cont[:tel], cont[:tel_link]  %>
      
      </li>
    <% end %>

  </ul>
</div>

<div class="row">
  <div class="heading-wrapper">
    <h2>Мы на карте</h2>
  </div>
</div>

